{
  "compilerOptions": {
    "target": "es2022",
    "lib": ["ES2020", "DOM", "DOM.Iterable"],
    "jsx": "preserve",
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noEmit": true,
    "module": "preserve",
    "moduleResolution": "bundler",
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "noErrorTruncation": false,
    "allowJs": true,
    "baseUrl": ".",
    "types": ["node", "react"]
  },
  "exclude": []
}
